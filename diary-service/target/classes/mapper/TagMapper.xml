<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dailybaro.diary.mapper.TagMapper">
    <select id="findAll" resultType="com.dailybaro.diary.model.Tag">
        SELECT tag_id, tag_name
        FROM tags
        ORDER BY tag_name
    </select>
</mapper> 